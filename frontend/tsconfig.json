{
    "compilerOptions": {
        "target": "es6",
        "module": "esNext",
        "moduleResolution": "Node",
        "outDir": "dist",
        "sourceMap": true,
        "noImplicitAny": false,
        "lib": ["es6", "dom", "es2017"],
        "jsx": "react",
        "baseUrl": "./src",
        "paths": {
            "*": ["*", "src/*"]
        },
        "esModuleInterop": true,
        "noUnusedLocals": false,
        "noUnusedParameters": true
    },
    "include": ["./src/**/*"]
}
